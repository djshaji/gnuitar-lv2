@prefix doap:  <http://usefulinc.com/ns/doap#> .
@prefix lv2:   <http://lv2plug.in/ns/lv2core#> .
@prefix rdf:   <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:  <http://www.w3.org/2000/01/rdf-schema#> .
@prefix units: <http://lv2plug.in/ns/extensions/units#> .

<http://shaji.in/plugins/gnuitar-distort>
	a lv2:Plugin ,
		lv2:AmplifierPlugin ;
	lv2:project <http://lv2plug.in/ns/lv2> ;

	doap:name "GNUitar Distortion" ;
	doap:license <http://opensource.org/licenses/isc> ;
	lv2:optionalFeature lv2:hardRTCapable ;
	lv2:port  [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 2 ;
		lv2:symbol "drive" ;
		lv2:name "Drive" ;
		lv2:default 55 ;
		lv2:minimum 1.0 ;
		lv2:maximum 100.0 ;
	] ,  [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 3 ;
		lv2:symbol "sat" ;
		lv2:name "Saturation" ;
		lv2:default 50 ;
		lv2:minimum 1.0 ;
		lv2:maximum 100.0 ;
	] ,  [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 4 ;
		lv2:symbol "level" ;
		lv2:name "Level" ;
		lv2:default 50.0 ;
		lv2:minimum 1.0 ;
		lv2:maximum 100.0 ;
	] , [
		a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 5 ;
		lv2:symbol "lowpass" ;
		lv2:name "Lowpass" ;
		lv2:default 350.0 ;
		lv2:minimum 1.0 ;
		lv2:maximum 3000.0 ;
	] , [
		a lv2:AudioPort ,
			lv2:InputPort ;
		lv2:index 0 ;
		lv2:symbol "in" ;
		lv2:name "In"
	] , [
		a lv2:AudioPort ,
			lv2:OutputPort ;
		lv2:index 1 ;
		lv2:symbol "out" ;
		lv2:name "Out"
	] .
